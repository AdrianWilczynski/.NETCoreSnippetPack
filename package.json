{
    "name": "asp-net-core-snippet-pack",
    "displayName": "ASP.NET Core Snippet Pack",
    "description": "A snippet pack for ASP.NET Core.",
    "publisher": "adrianwilczynski",
    "version": "1.0.1",
    "icon": "logo.png",
    "engines": {
        "vscode": "^1.31.0"
    },
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "Snippets",
        "Snippet",
        "ASP.NET Core",
        ".NET Core",
        ".NET",
        "C#",
        "CSharp",
        "Razor"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "csharp",
                "path": "./snippets/csharp.json"
            },
            {
                "language": "aspnetcorerazor",
                "path": "./snippets/razor.json"
            },
            {
                "language": "razor",
                "path": "./snippets/razor.json"
            },
            {
                "language": "json",
                "path": "./snippets/appsettings.json"
            },
            {
                "language": "xml",
                "path": "./snippets/csproj.json"
            }
        ]
    }
}
